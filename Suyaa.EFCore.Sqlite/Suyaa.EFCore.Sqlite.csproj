<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>1.6.2311.32</Version>
    <Authors>Wenling Lianyi Network Technology Co., Ltd.</Authors>
    <Description>舒雅套件中服务于Sqlite数据库EFCore框架的辅助开发组件</Description>
    <PackageProjectUrl>https://github.com/suyaas/suyaa</PackageProjectUrl>
    <RepositoryUrl>https://github.com/suyaas/suyaa</RepositoryUrl>
    <PackageIcon>suyaa_128.png</PackageIcon>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Copyright>Copyright © Wenling Lianyi Network Technology Co., Ltd. 2014</Copyright>
    <PackageTags>orm,efcore,sqlite</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\icon\suyaa_128.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="6.0.16" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Suyaa.Data.Sqlite\Suyaa.Data.Sqlite.csproj" />
    <ProjectReference Include="..\Suyaa.EFCore\Suyaa.EFCore.csproj" />
  </ItemGroup>

</Project>
