<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>
    <Version>1.8.2312.36</Version>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>Wenling Lianyi Network Technology Co., Ltd.</Authors>
    <Description>IoC container component in Suyaa development kit.</Description>
    <Copyright>Copyright © Wenling Lianyi Network Technology Co., Ltd. 2014</Copyright>
    <PackageProjectUrl>https://github.com/suyaas/suyaa</PackageProjectUrl>
    <RepositoryUrl>https://github.com/suyaas/suyaa</RepositoryUrl>
    <PackageIcon>Suyaa_red_128.png</PackageIcon>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <PackageTags>ioc,di</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\icon\Suyaa_red_128.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Suyaa\Suyaa.csproj" />
  </ItemGroup>

</Project>
